        <servlet-name>Servlet6</servlet-name>
        <url-pattern>/Servlet6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadServ</servlet-name>
        <url-pattern>/ReadServ</url-pattern>
    </servlet-mapping>
